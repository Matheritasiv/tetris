            VALUE "PrivateBuild", ""
            VALUE "ProductName", "Tetris"
            VALUE "ProductVersion", "2.0"
            VALUE "SpecialBuild", ""
        }
    }
    BLOCK "VarFileInfo"
    {
        VALUE "Translation", 0x0800, 0xC433
    }
