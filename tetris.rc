#define IDI_LOGO        100
#define IDI_LOGO_SMALL  101
#define IDI_START       102
#define IDI_RECORD      103
#define IDI_REPLAY      104
#define IDI_PAUSE       105
#define IDI_HELP        106
#define IDI_TERM        107
#define IDB_LANG        200
#define IDB_INFO        201

#ifndef __IN_CODE__
#include<windows.h>

// Icon resources
LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
IDI_LOGO        ICON    ".\\tetris.ico"
